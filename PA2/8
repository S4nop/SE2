		<script>
			var slideIntervalTime=8000;
			$("#sliderBox_>.slide_").each(function(index){
				$("<li onClick=\"showSlide("+index+")\"><button onfocus=\'blur()\'>•</button></li>").appendTo("#sliderBox_>.slide_button_");
			});
			$("#sliderBox_>.slide_").css("display","none").eq(0).css({"display":"block","z-index":998});
			$("#sliderBox_>.slide_button_>li:eq(0)>button").css("color","blue");
			
			setInterval(function(){
				if($("#sliderBox_>.slide_").is(":animated")) return 0;
				showSlide();
			},slideIntervalTime);
			
			function showSlide(index){
				if(typeof(index)==='undefined')index=$("#sliderBox_>.slide_:visible").index()+1;
				if($("#sliderBox_>.slide_").length==index) index=0;
				var prevSlide=$("#sliderBox_>.slide_:visible");
				var nextSlide=$("#sliderBox_>.slide_:eq("+index+")");
				prevSlide.css("z-index",997);
				nextSlide.css("z-index",998);
				nextSlide.fadeIn(1000,function(){
					prevSlide.css("z-index",998).hide();
				});
				$("#sliderBox_>.slide_button_>li>button").css("color","white");
				$("#sliderBox_>.slide_button_>li:eq("+index+")>button").css("color","blue");
			}
		</script>
