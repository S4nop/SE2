<script>
	$(document).ready(function(){
		   if($("#sliderBox_popup>.slide_").length > 0){
		       $("#sliderBox_popup").css("height", "5.26vw");
		   }
	});
	var slideIntervalTime=5000;
	var current = 0;
	var num_pop = 0;
	var start_num = 0;
	$("#sliderBox_popup>.slide_").each(function(index){
		num_pop++;	
	});
	
	start_num = Math.floor(Math.random() * num_pop);
	current = start_num;
	
	$("#sliderBox_popup>.slide_").each(function(index){
		console.log("excute");
		console.log(start_num);
		if(num_pop != (0 || 1) && index == 0)
			$("<li onClick=\"prevSlide("+index+")\")\"><button onfocus=><</button></li>").appendTo("#sliderBox_popup>.slide_button_prev");
		//$("<li onClick=\"showPopupSlide("+index+")\"><button onfocus=\'blur()\'>•</button></li>").appendTo("#sliderBox_popup>.slide_button_");		
		if(num_pop != (0 || 1) && index == (num_pop - 1))
			$("<li onClick=\"nextSlide("+index+")\"><button onfocus=>></button></li>").appendTo("#sliderBox_popup>.slide_button_next");
	});
	
	$("#sliderBox_popup>.slide_").css("display","none").eq(start_num).css({"display":"block","z-index":998});
	//$("#sliderBox_popup>.slide_button_>li:eq(start_num)>button").css("color","blue");
	
	if(num_pop != (0 || 1)){
		setInterval(function(){
			if($("#sliderBox_popup>.slide_").is(":animated")) return 0;
			nextSlide(start_num);
		},slideIntervalTime);
	}
	
	function prevSlide(index){
		if( current != 0 )
			current--;
		else
			current = num_pop - 1;
		showPopupSlide(current);
		return current;
	}
	function nextSlide(index){
		if(current != (num_pop - 1))
			current++;
		else
			current = 0;
		showPopupSlide(current);
		return current;
	}
	
	function showPopupSlide(index){
		if(typeof(index)==='undefined')index=$("#sliderBox_popup>.slide_:visible").index();
		if($("#sliderBox_popup>.slide_").length==index) index=0;
		var prevSlide=$("#sliderBox_popup>.slide_:visible");
		var nextSlide=$("#sliderBox_popup>.slide_:eq("+index+")");
		prevSlide.css("z-index",997);
		nextSlide.css("z-index",998);
		nextSlide.fadeIn(500,function(){
			prevSlide.css("z-index",998).hide();
		});
		$("#sliderBox_popup>.slide_button_>li>button").css("color","white");
		$("#sliderBox_popup>.slide_button_>li:eq("+index+")>button").css("color","blue");
		console.log(current);
		current = index;
		console.log(current);
	}
	function closePopup(){
		console.log("closePopup");
		$("#sliderBox_popup").css("height", 0);
	}
	
</script>
