<script>
	function removeValue()
	{
		document.getElementById('keyword').value="";
		return ;
	}

	$(document).ready(function(){
			
		$('#menuIntro').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuIntroSub').animate({ height: "show" }, 100);
		});
		$('#menuIntrogra').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuIntrograSub').animate({ height: "show" }, 100);
		});
		$('#menuEnter').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuEnterSub').animate({ height: "show" }, 100);
		});
		$('#menuFaculty').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuFacultySub').animate({ height: "show" }, 100);
		});
		$('#menuInfo').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuInfoSub').animate({ height: "show" }, 100);
		});
		$('#menuOrg').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuOrgSub').animate({ height: "show" }, 100);
		});
		$('#menuOffice').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuOfficeSub').animate({ height: "show" }, 100);
		});
		$('#menuSquare').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuSquareSub').animate({ height: "show" }, 100);
		});
		$('#menuService').mouseenter(function() {
			$('#subMenu > div').css("display", "none");
		    $('#menuServiceSub').animate({ height: "show" }, 100);
		});
		
		$('#mainsub').mouseleave(function(){
			$('#subMenu > div').animate({height: "hide"}, 100);
		});
		
	});	

</script>


